<odoo noupdate="0">


<record id="donation_recurrency_seq" model="ir.sequence">
    <field name="name">Donation Recurrency</field>
    <field name="code">donation.recurrency</field>
    <field name="prefix">DON-REC-%(range_year)s-</field>
    <field name="padding">4</field>
    <field name="use_date_range" eval="True" />
    <field name="company_id" eval="False" />
</record>

<record id="donation_seq" model="ir.sequence">
  <field name="name">Donation</field>
  <field name="code">donation.donation</field>
  <field name="prefix">DON-%(range_year)s-</field>
  <field name="padding">4</field>
  <field name="use_date_range" eval="True" />
  <field name="company_id" eval="False" />
</record>

</odoo>
